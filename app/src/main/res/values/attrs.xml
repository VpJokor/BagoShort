<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlidingIndicatorBar">
        <attr name="sib_bar_height" format="dimension|reference"/>
        <attr name="sib_bar_color" format="color|reference"/>
        <attr name="sib_bending_height" format="dimension|reference"/>
        <attr name="sib_bending_ratio" format="float"/>
        <attr name="sib_bending_direction" format="enum">
            <enum name="down" value="0"/>
            <enum name="up" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="LikeView">
        <attr name="defaultColor" format="color"/>
        <attr format="color" name="checkedColor"/>
        <attr format="color" name="ringColor"/>
        <attr format="dimension" name="cirRadius"/>
        <attr format="integer" name="cycleTime"/>
        <attr format="integer" name="unSelectCycleTime"/>
        <attr format="reference" name="defaultLikeIconRes"/>
        <attr format="reference" name="checkedLikeIconRes"/>
        <attr format="float" name="lrGroupCRatio"/>
        <attr format="float" name="lrGroupBRatio"/>
        <attr format="float" name="bGroupACRatio"/>
        <attr format="float" name="tGroupBRatio"/>
        <attr format="float" name="innerShapeScale"/>
        <attr format="integer" name="dotSizeScale"/>
        <attr format="boolean" name="allowRandomDotColor"/>
    </declare-styleable>
</resources>